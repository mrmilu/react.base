    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "prettier": "2.6.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "resolutions": {
    "graphql": "15.5.3"
  }
