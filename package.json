{
  "name": "react_base",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "lint": "eslint --fix --ext js,ts,tsx src",
    "check-types": "tsc --project tsconfig.json",
    "prepare": "husky install",
    "commit": "commit",
    "graphql-schema": "apollo client:download-schema schema.json",
    "graphql-types": "apollo client:codegen --target=typescript --localSchemaFile=schema.json --globalTypesFile=src/__generated__/globalTypes.d.ts",
    "graphql-possible-types": "node generate_possible_types.js",
    "graphql": "yarn graphql-schema --endpoint=https://graphqlzero.almansi.me/api && yarn graphql-possible-types && yarn graphql-types",
    "e2e:run": "cypress run",
    "e2e:open": "cypress open"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.6.6",
    "@front_web_mrmilu/hooks": "mrmilu/front_web_mrmilu#@front_web_mrmilu/hooks-v1.2.0",
