    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "http-proxy-middleware": "2.0.6",
    "husky": "8.0.1",
    "lint-staged": "13.0.1",
    "prettier": "2.6.2"
  },
  "resolutions": {
    "graphql": "15.5.3",
    "@types/react": "18.x"
  }
